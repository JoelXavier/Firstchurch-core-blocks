{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "firstchurch/event-list",
  "version": "1.0.0",
  "title": "Event List (Manual)",
  "category": "firstchurch",
  "icon": "calendar-alt",
  "description": "A manually curated list of events. Drag and drop Event Item blocks inside.",
  "attributes": {
    "sectionTitle": {
      "type": "string",
      "default": ""
    },
    "sectionSubheader": {
      "type": "string",
      "default": ""
    },
    "viewAllText": {
      "type": "string",
      "default": "See All Events"
    },
    "viewAllLink": {
      "type": "string",
      "default": ""
    },
    "headerColor": {
      "type": "string"
    },
    "cornerRadius": {
      "type": "string",
      "default": "0px"
    },
    "viewAllColor": {
      "type": "string"
    },
    "cardGap": {
      "type": "string",
      "default": "0px"
    }
  },
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ],
    "color": {
      "background": true,
      "text": true
    }
  },
  "textdomain": "first-church-core-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}