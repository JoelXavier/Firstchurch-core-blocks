{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "firstchurch/paragraph",
  "version": "1.0.1",
  "title": "FC Paragraph",
  "category": "firstchurch",
  "icon": "text",
  "description": "A flexible paragraph block tailored for First Church.",
  "keywords": [
    "text",
    "paragraph"
  ],
  "supports": {
    "anchor": true,
    "className": true,
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontWeight": true,
      "__experimentalFontStyle": true,
      "__experimentalLetterSpacing": true,
      "__experimentalTextTransform": true,
      "__experimentalTextDecoration": true,
      "__experimentalFontFamily": true
    },
    "color": {
      "background": true,
      "text": true,
      "gradients": true,
      "link": true
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "__experimentalSelector": "p"
  },
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "p",
      "default": ""
    }
  },
  "textdomain": "first-church-core-blocks",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}